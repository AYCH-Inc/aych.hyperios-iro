{
  "name": "IrohaSwift",
  "version": "0.0.9",
  "summary": "Easy create signature of Iroha Blockchain",
  "homepage": "http://www.soramitsu.co.jp/",
  "license": "Apache License Version 2.0",
  "authors": {
    "Soramitsu Co., Ltd.": "info@soramitsu.co.jp"
  },
  "source": {
    "git": "https://github.com/soramitsu/IrohaSwift.git",
    "tag": "0.0.9"
  },
  "platforms": {
    "ios": "9.0"
  },
  "source_files": [
    "lib/**/*.{h,c}",
    "IrohaSwift/*.{h,swift}"
